<tr>
    <td><%=result[:id]%></td>
    <td><%=image_tag result[:avatar], size: "40x40"%></td>
    <td><%=result[:last_name]%></td>
    <td><%=result[:first_name]%></td>
    <td><%=result[:email]%></td>
    <td><%= link_to "Save to DB", create_user_path(id: result[:id], avatar: result[:avatar], first_name: result[:first_name], last_name: result[:last_name], email: result[:email]), class: "add", remote: true %></td>
</tr>
